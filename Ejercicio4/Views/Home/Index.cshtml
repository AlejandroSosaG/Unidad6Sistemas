@{
    ViewData["Title"] = "Home Page";
}

<style>
    td {
        border: 1px solid black;
        padding: 1px;
        width: 100px;
    }
</style>

<table>
    @for (int i = 1; i <= 12; i++) {
        <tr>
            @for (int j = 1; j <= 10; j++) {
                <td>@i x @j = @(i * j)</td>
            }
        </tr>
    }
</table>